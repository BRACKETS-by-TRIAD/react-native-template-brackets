{
  "name": "reactNativeTemplateBrackets",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "dev": "react-native start",
    "run-android:stagingRelease": "ENVFILE=.env.staging react-native run-android --variant=release",
    "run-android:productionRelease": "ENVFILE=.env.prod react-native run-android --variant=release",
    "run-ios:stagingRelease": "react-native run-ios --scheme STAGING --configuration Release",
    "run-ios:productionRelease": "react-native run-ios --scheme PRODUCTION --configuration Release",
    "postinstall": "patch-package",
    "svg:generate": "npx @svgr/cli -d app/assets/svg/generated app/assets/svg --native --ext tsx --typescript",
    "config:gen": "node scripts/buildConfig.js",
    "tsc:check": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "android-release": "npx react-native run-android --variant=release",
    "android-bundle-release": "./android/gradlew bundleRelease",
    "extract": "lingui extract",
    "compile": "lingui compile"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn tsc:check && yarn lint",
      "pre-commit": "yarn tsc:check && yarn lint --fix"
    }
  },
  "dependencies": {
    "@lingui/react": "^3.4.0",
    "@react-native-async-storage/async-storage": "^1.13.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.3",
    "@react-navigation/native": "^5.9.0",
    "@react-navigation/stack": "^5.13.0",
    "formstate": "^2.0.0",
    "lodash": "^4.17.20",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.2.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-config": "^1.4.2",
    "react-native-elements": "^3.1.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-reanimated": "^2.0.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.16.1",
    "react-native-styled.macro": "^0.11.0",
    "react-native-svg": "^12.1.0",
    "react-native-unimodules": "^0.12.0",
    "react-native-vector-icons": "^8.1.0",
    "rn-handy-utils": "^0.2.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@lingui/cli": "^3.4.0",
    "@lingui/macro": "^3.4.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.168",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-macros": "^3.0.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-10x": "^1.5.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-reanimated": "^1.2.1",
    "eslint-plugin-ts-exports": "^1.2.1",
    "husky": "4",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
